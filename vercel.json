{
  "version": 2,
  "builds": [
    {
      "src": "test-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/test-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/test-server.js"
    }
  ],
  "functions": {
    "test-server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  }
}